html,
body {
  height: 100%;
  overflow: hidden; }

nav {
  background-color: black;
  color: white;
  height: 60px;
  line-height: 60px;
  padding: 0 24px; }
  nav .left {
    display: inline-block; }
    nav .left .title {
      color: #ffffff;
      padding-left: 14px; }
  nav .right {
    float: right; }
    nav .right .welcome-msg {
      padding-right: 26px;
      color: #9b9b9d; }
