/** 手机媒体查询 **/
@media screen and (max-width: 768px) {
  html,
  body {
    font-size: 14px;
    margin: 0;
    padding: 0;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    background-color: #fff; } }

